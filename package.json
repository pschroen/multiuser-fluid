{
  "name": "multiuser-fluid",
  "version": "1.1.4",
  "description": "An experiment to combine UI and data visualization elements in a multiuser environment.",
  "homepage": "https://multiuser-fluid.glitch.me/",
  "keywords": [
    "node",
    "glitch",
    "multiuser",
    "fluid"
  ],
  "author": "pschroen",
  "private": true,
  "repository": {
    "url": "https://glitch.com/edit/#!/multiuser-fluid"
  },
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "main": "server.js",
  "scripts": {
    "prestart": "rollup -c",
    "build": "rollup -c",
    "dev": "concurrently --names \"ROLLUP,HTTP\" -c \"bgBlue.bold,bgGreen.bold\" \"rollup -c -w -m inline\" \"node server.js\"",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.17.3",
    "express-ws": "^5.0.2",
    "three": "^0.138.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "concurrently": "^7.0.0",
    "rollup": "^2.68.0",
    "rollup-plugin-bundleutils": "^1.2.4"
  }
}
